    padding 20px
    border-bottom 1px solid common-border
    font-size 1.5rem
    color sectionTitle
    span{
      font-weight 500
      margin 0 5px
    }
  }
  .student-result{
    padding 20px
    .spec-select{
      margin-bottom 30px
      display flex
      align-items center
      width 500px
      .label{
        font-size 1.5rem
